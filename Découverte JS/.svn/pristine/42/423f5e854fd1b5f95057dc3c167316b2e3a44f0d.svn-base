//Etape 2

function	make_custom_array(start, stop,step)
{
    var	tab = [];
    if (start < stop)
    {
	for (i = start; i <= stop; i += step)
	{
	    tab[tab.length] = i;
	    start += step;
	}
	return (tab);
    }
    else if (step <= 0 || (isNaN(start, step, stop == true)))
	return (tab);
    else if (start > stop)
    {
	for (i = start; i >= stop; i -= step)
	{
	    tab[tab.length] = i;
	}
	return (tab);
    }
}